SELECT title,release_year FROM films WHERE (release_year >=1990 AND release_year <= 1999);
SELECT title, release_year FROM films WHERE (release_year >= 1990 AND release_year < 2000) AND (language='Spanish' OR language='French')
SELECT title, release_year FROM films WHERE (release_year >= 1990 AND release_year < 2000) AND (language = 'French' OR language = 'Spanish') AND gross> 2000000
